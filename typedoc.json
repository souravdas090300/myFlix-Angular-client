{
  "entryPoints": ["src/**/*.ts"],
  "entryPointStrategy": "expand",
  "out": "docs",
  "exclude": [
    "**/*.spec.ts",
    "**/*.test.ts",
    "**/test-*.ts",
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "name": "myFlix Angular Client Documentation",
  "theme": "default",
  "includeVersion": true,
  "excludePrivate": false,
  "excludeProtected": false,
  "disableSources": false,
  "tsconfig": "tsconfig.json"
}
